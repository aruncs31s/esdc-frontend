# Go Backend API Configuration
VITE_API_BASE_URL=https://esdc-backend.onrender.com
VITE_API_TIMEOUT=30000

# Alternative local development backend
# VITE_API_BASE_URL=http://localhost:8080

# Environment
VITE_ENV=development

# Backend API Endpoints (for reference)
# Authentication: /api/auth/login, /api/auth/register
# Admin: /api/admin/*
# Projects: /api/projects/*
# Challenges: /api/challenges/*
# Users: /api/users/*

# Feature Flags
VITE_ENABLE_CHATBOT=true
VITE_ENABLE_EVENTS=true
VITE_ENABLE_CHALLENGES=true
VITE_ENABLE_RESOURCES=true

# Analytics (Optional)
VITE_ANALYTICS_ID=

# OAuth (Optional)
VITE_GITHUB_CLIENT_ID=
